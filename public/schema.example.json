{
	"nodes": [
		{
			"node": "Drug",
			"example": {
				"href": {
					"pathname": "/",
					"query": {
						"start": "Drug",
						"start_term": "Esmolol"
					}
				}
			},
			"palette": {
				"name": "oranges",
				"main": "#ffb74d",
				"contrastText": "#000"
			},
			"display": [
				{
					"label": "Drug ID",
					"text": "${id}",
					"type": "link",
					"href": "https://pubchem.ncbi.nlm.nih.gov/compound/${id.replace('CID:', '')}"
				},
				{
					"label": "GDLPA",
					"text": "${label}",
					"type": "link",
					"href": "https://cfde-gene-pages.cloud/drug/${label}?CF=false&PS=true"
				}
			]
		},
		{
			"node": "Gene",
			"example": {
				"href": {
					"pathname": "/",
					"query": {
						"start": "Gene",
						"start_term": "TAT"
					}
				}
			},
			"palette": {
				"name": "greens",
				"main": "#aed581",
				"contrastText": "#000"
			},
			"display": [
				{
					"label": "Gene ID",
					"text": "${id}",
					"type": "link",
					"href": "https://www.ncbi.nlm.nih.gov/gene/${id}"
				},
				{
					"label": "Aliases",
					"text": "${Aliases.split('|').join(', ')}",
					"type": "text"
				},
				{
					"label": "Alternative IDs",
					"text": "${Alt_IDs.split('|').join(', ')}",
					"type": "text"
				},
				{
					"label": "ARCHS4",
					"text": "${label}",
					"type": "link",
					"href": "https://maayanlab.cloud/archs4/gene/${label}"
				},
				{
					"label": "GDLPA",
					"text": "${label}",
					"type": "link",
					"href": "https://cfde-gene-pages.cloud/gene/${label}?CF=false&PS=true&Ag=true"
				}
			]
		},
		{
			"node": "Glycoconjugate",
			"example": {
				"href": {
					"pathname": "/",
					"query": {
						"start": "Glycoconjugate",
						"start_term": "Asn-GlcNAc..."
					}
				}
			},
			"palette": {
				"name": "purples",
				"main": "#b39ddb",
				"contrastText": "#000"
			},
			"display": [
				{
					"label": "Amino acid",
					"text": "${amino_acid}",
					"type": "text"
				},
				{
					"label": "Glycan",
					"text": "${carb}",
					"type": "text"
				}
			]
		},
		{
			"node": "Metabolite",
			"example": {
				"href": {
					"pathname": "/",
					"query": {
						"start": "Metabolite",
						"start_term": "L-Glutamate"
					}
				}
			},
			"palette": {
				"name": "cyans",
				"main": "#26c6da",
				"contrastText": "#000"
			},
			"display": [
				{
					"label": "Metabolite ID",
					"text": "${id}",
					"type": "link",
					"href": "https://www.genome.jp/entry/${id}"
				},
				{
					"label": "Synonyms",
					"text": "${synonyms.join(', ')}",
					"type": "text"
				}
			]
		}
	],
	"edges": [
		{
			"match": ["physical association", "direct interaction"],
			"order": ["Confidence_Values", "DESC"],
			"display": [
				{
					"label": "ID",
					"text": "${id}",
					"type": "text"
				},
				{
					"label": "Source Label",
					"text": "${source_label}",
					"type": "text"
				},
				{
					"label": "Target Label",
					"text": "${target_label}",
					"type": "text"
				},
				{
					"label": "Source Database",
					"text": "${Source_Database}",
					"type": "text"
				},
				{
					"label": "Interaction Detection Method",
					"text": "${Interaction_Detection_Method}",
					"type": "text"
				},
				{
					"label": "Pubmed",
					"text": "${Publication_Identifiers}",
					"type": "text"
				},
				{
					"label": "Publication First Author",
					"text": "${Publication_1st_Author}",
					"type": "text"
				},
				{
					"label": "Confidence Value",
					"text": "${Confidence_Values}",
					"type": "text"
				},
				{
					"label": "Resource",
					"text": "BioGRID",
					"href": "https://thebiogrid.org/",
					"type": "link"
				}	
			]
		},
		{
			"match": ["negatively correlated"],
			"palette": {
				"main": "#f8bbd0"
			},
			"order": ["correlation_coefficient", "ASC"],
			"display": [
				{
					"label": "ID",
					"text": "${id}",
					"type": "text"
				},
				{
					"label": "Source Label",
					"text": "${source_label}",
					"type": "text"
				},
				{
					"label": "Target Label",
					"text": "${target_label}",
					"type": "text"
				},
				{
					"label": "Source Database",
					"text": "${Source_Database}",
					"type": "text"
				},
				{
					"label": "Correlation Coefficient",
					"text": "${correlation_coefficient}",
					"type": "number"
				},
				{
					"label": "Resource",
					"text": "ARCHS4",
					"href": "https://maayanlab.cloud/archs4/",
					"type": "link"
				}	
			]
		},
		{
			"match": ["positively correlated"],
			"palette": {
				"main": "#4fc3f7"
			},
			"order": ["correlation_coefficient", "DESC"],
			"display": [
				{
					"label": "ID",
					"text": "${id}",
					"type": "text"
				},
				{
					"label": "Source Label",
					"text": "${source_label}",
					"type": "text"
				},
				{
					"label": "Target Label",
					"text": "${target_label}",
					"type": "text"
				},
				{
					"label": "Source Database",
					"text": "${Source_Database}",
					"type": "text"
				},
				{
					"label": "Correlation Coefficient",
					"text": "${correlation_coefficient}",
					"type": "number"
				},
				{
					"label": "Resource",
					"text": "ARCHS4",
					"href": "https://maayanlab.cloud/archs4/",
					"type": "link"
				}		
			]
		},
		{
			"match": ["downregulates"],
			"palette": {
				"main": "#f8bbd0"
			},
			"order": ["mean_CD_coefficient", "ASC"],
			"display": [
				{
					"label": "ID",
					"text": "${id}",
					"type": "text"
				},
				{
					"label": "Source Label",
					"text": "${source_label}",
					"type": "text"
				},
				{
					"label": "Target Label",
					"text": "${target_label}",
					"type": "text"
				},
				{
					"label": "Source Database",
					"text": "${Source_Database}",
					"type": "text"
				},
				{
					"label": "Correlation Coefficient",
					"text": "${correlation_coefficient}",
					"type": "number"
				},
				{
					"label": "Resource",
					"text": "SigCom LINCS",
					"href": "https://maayanlab.cloud/sigcom-lincs/",
					"type": "link"
				}	
			]
		},
		{
			"match": ["upregulates"],
			"palette": {
				"main": "#4fc3f7"
			},
			"order": ["mean_CD_coefficient", "DESC"],
			"display": [
				{
					"label": "ID",
					"text": "${id}",
					"type": "text"
				},
				{
					"label": "Source Label",
					"text": "${source_label}",
					"type": "text"
				},
				{
					"label": "Target Label",
					"text": "${target_label}",
					"type": "text"
				},
				{
					"label": "Source Database",
					"text": "${Source_Database}",
					"type": "text"
				},
				{
					"label": "Correlation Coefficient",
					"text": "${correlation_coefficient}",
					"type": "number"
				},
				{
					"label": "Resource",
					"text": "SigCom LINCS",
					"href": "https://maayanlab.cloud/sigcom-lincs/",
					"type": "link"
				}	
			]
		},
		{
			"match": ["Metabolite-Gene"],
			"display": [
				{
					"label": "Source Label",
					"text": "${source_label}",
					"type": "text"
				},
				{
					"label": "Target Label",
					"text": "${target_label}",
					"type": "text"
				},
				{
					"label": "Resource",
					"text": "Metabolomics Workbench",
					"href": "https://www.metabolomicsworkbench.org/",
					"type": "link"
				}	
			]
		}
	],
	"color": {
		"Confidence_Values": {
			"on": "edge",
			"field": ["Confidence_Values", "DESC"]
		}
	},
	"tooltip": {
		"Gene": [
			{
				"label": "Gene ID",
				"text": "${id}",
				"type": "link",
				"href": "https://www.ncbi.nlm.nih.gov/gene/${id}"
			},
			{
				"label": "Aliases",
				"text": "${Aliases.split('|').join(', ')}",
				"type": "text"
			},
			{
				"label": "Alternative IDs",
				"text": "${Alt_IDs.split('|').join(', ')}",
				"type": "text"
			},
			{
				"label": "ARCHS4",
				"text": "${label}",
				"type": "link",
				"href": "https://maayanlab.cloud/archs4/gene/${label}"
			},
			{
				"label": "GDLPA",
				"text": "${label}",
				"type": "link",
				"href": "https://cfde-gene-pages.cloud/gene/${label}?CF=false&PS=true&Ag=true"
			}
		],
		"Drug": [
			{
				"label": "Drug ID",
				"text": "${id}",
				"type": "link",
				"href": "https://pubchem.ncbi.nlm.nih.gov/compound/${id.replace('CID:', '')}"
			},
			{
				"label": "GDLPA",
				"text": "${label}",
				"type": "link",
				"href": "https://cfde-gene-pages.cloud/drug/${label}?CF=false&PS=true"
			}
		],
		"Relation": [
			{
				"label": "ID",
				"text": "${id}",
				"type": "text"
			},
			{
				"label": "Source Label",
				"text": "${source_label}",
				"type": "text"
			},
			{
				"label": "Target Label",
				"text": "${target_label}",
				"type": "text"
			},
			{
				"label": "Source Database",
				"text": "${Source_Database}",
				"type": "text"
			},
			{
				"label": "Interaction Detection Method",
				"text": "${Interaction_Detection_Method}",
				"type": "text"
			},
			{
				"label": "Publication First Author",
				"text": "${Publication_1st_Author}",
				"type": "text"
			},
			{
				"label": "Pubmed",
				"text": "${Publication_Identifiers}",
				"type": "text"
			},
			{
				"label": "Alternative IDs",
				"text": "${Alt_IDs.split('|').join(',')}",
				"type": "text"
			},
			{
				"label": "Confidence Value",
				"text": "${Confidence_Values}",
				"type": "text"
			},
			{
				"label": "Correlation Coefficient",
				"text": "${correlation_coefficient}",
				"type": "number"
			},
			{
				"label": "Mean CD Coefficient",
				"text": "${mean_CD_coefficient}",
				"type": "number"
			}
		]
	},
	"header": {
		"title": "Gene-Centric CFDE Knowledge Graph",
		"subheader": [
			{
				"label": "LINCS",
				"icon": "${PREFIX}/static/resources/lincs.gif"
			},
			{
				"label": "Metabolomics Workbench",
				"icon": "${PREFIX}/static/resources/metabolomics.png"
			},
			{
				"label": "GlyGen",
				"icon": "${PREFIX}/static/resources/glygen.png"
			},
			{
